[attr]lfs filter=lfs diff=lfs merge=lfs
[attr]lfstext filter=lfs diff=lfstext merge=text -text
# Unreal Engine file types.
*.uasset lfs
*.umap lfs
*.locres lfs
*.locmeta lfs
# Steam Audio files
*.phononscene lfs
*.probebox lfs
*.probebatch lfs
*.bakedsources lfs
# Binaries
*.exe lfs
*.dll lfs
*.rcc lfs
# FMOD
*.bank lfs
*.wav lfs
*.mp3 lfs
*.ogg lfs
*.flac lfs
# Icons
*.ico lfs
*.icns lfs
# Movies
*.bk2 lfs
